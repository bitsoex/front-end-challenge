  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "highcharts": "^6.1.0",
    "highcharts-react-official": "^1.1.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "npm-run-all": "^4.1.3",
    "numeral": "^2.0.6",
    "rc-menu": "^7.0.3",
